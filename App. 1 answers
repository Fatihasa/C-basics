Ans.6
#include <iostream>
#include<ctime>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(int argc, char** argv) {
	
		cout<<"Typ the size of pattern 0.:";
	int size;
	int j;
	cin>>size;
	char a='V';
	int size1=size;
	int size2=size;
	for(int i=0;i<size;i++){
		for(int i=0;i<size1;i++){
			cout<<a;
		}
		cout<<endl;
		size1--;
		}
	size1+=2;
		
	for(int j=0;j<size-1;j++){
		for(int j=0;j<size1;j++){
			cout<<a;			
		}
		cout<<endl;	
		size1++;			
	}
}
