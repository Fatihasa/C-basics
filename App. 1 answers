Ans.7
#include<iostream>
#include<cmath>
#include<cstdlib>

using namespace std;

int main(int argc, char** argv){
	
	int size1,size2;
	int rows=size1;
	cout<<"Typ the number of rows at table:";
	cin>>size1;
	cout<<"Typ the number of coloums at table:";
	cin>>size2;
	int a=1;
	
	for(int i=0;i<=size1-1;i++){
		
		for(int i=0;i<size2;i++){
			
			cout<<a++;
			cout<<" ";
		}
		cout<<endl;
		a=a-(size2-1);
	}
}
