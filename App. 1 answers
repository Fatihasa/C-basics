#include <iostream>
#include<ctime>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(int argc, char** argv) {
	
	cout<<"Typ the size of pattern 0.:";
	int size;
	cin>>size;
	char a='A';
	
	for(int i=0;i<size;i++){
		
		for(int i=0;i<size;i++){
			
			cout<<a;
			
		}
		
		cout<<endl;

	}
}
