#include <iostream>
#include<ctime>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(int argc, char** argv) {
	
	int x;
	int s;
	cout<<"Typ the sequence of X :"<<endl;
	cin>>x;
	cout<<"Typ the size of sequense :"<<endl;
	cin>>s;
	
	int sum=0;
	
	for(int i=0;i<s;i++){
		
		sum=sum+x;
		
		cout<<sum<<" ";
		
	}
}	
