Ans.10
#include <iostream>

using namespace std;

int main(int argc, char** argv) {
	
	
	int size;
	cout << "Type the size of the matrix\n";
	int a=1;
	cin >> size;
	cout << "Type your numbers\n";

	for(int i=0; i<size; i++){
		
		for(int j=0;j<size;j++){
			
			if(i==j){
			cout<<a;
			a++;
			}
			else{
			cout<<"0";
			}
		}
		cout<<endl;
	}
}
