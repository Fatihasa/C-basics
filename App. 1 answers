Ans.1

#include <iostream>
#include<ctime>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(int argc, char** argv) {
	
	int x;
	cout<<"Typ the sequence of X"<<endl;
	cin>>x;
	int size=10;
	int sum=0;
	
	for(int i=0;i<size;i++){
		
		sum=sum+x;	
		cout<<sum<<"x"<<" ";
		
	}
	
}
