Ans.11
#include<iostream>
#include<cmath>
#include<cstdlib>

using namespace std;

int main(int argc, char** argv){
	
	int size1,size2;
	int rows=size1;
	cout<<"Typ the number of rows at table:";
	cin>>size1;
	cout<<"Typ the number of coloums at table:";
	cin>>size2;
	
	int c=1;
	int d=2;
	int k=2;
	int f=0;
	
	for(int i=1;i<=size1;i++){
		
		for(int j=1;j<=size2;j++){
			
			if(i==j || j>i){
				cout<<c;
				cout<<" ";
				c++;
			}
			else if(i>j){
				cout<<d;
				cout<<" ";
				d++;
			}
		}
		c=1;
		cout<<endl;
	}			
}
----------------------------------------------------------------
Ans.12
#include<iostream>
#include<cstdlib>

using  namespace std;

int main(int argc,char** argv){
	
	int a=5;
	int b=5;
	int c=4;
	int d=4;
	
	for(int i=0;i<a;i++){
		for(int j=0;j<b;j++){
			if(j>i){
				cout<<" ";
			}
			else {
				cout<<"*";
			}
		}
	cout<<endl;
	}
	for(int i=0;i<d;i++){
		for(int j=0;j<c;j++){
			
			cout<<"*";
		}
	cout<<endl;
	c--;
	}
}

























