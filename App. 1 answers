	cout << "Type your numbers\n";
	int matrix[size][size];
	
	for(int i=0; i<size; i++){
		
		for(int j=0;j<size;j++){
			
		if(j>i){
			cout<<"0";
		}
		else{
			cout<<a;
		}
		}
		cout<<endl;
		a++;
	}
}
