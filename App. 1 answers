#include <iostream>
#include<ctime>
#include<cstdlib>
#include<cmath>

using namespace std;

int main(int argc, char** argv) {
	
	float number;
	
	cout<<"Typ a number between 1 to 100. I will dived it to 5 :"<<endl;
	cin>>number;
	
	float sum=0;
	
	if(1<number & number<100){
		
		sum=number/5;
		cout<<sum;
	}
	else{
		cout<<"you typed a different number or charcter";
	}
	
}	
